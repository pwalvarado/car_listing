<% cache(expires_in: 10.minutes, race_condition_ttl: 10.seconds) do %>
<script type="application/json" id="bootstrapped-listings">
  {"listings":
      <%=
            render('listings/index.json.jbuilder').html_safe %>,
   "subdivisions":
      <%=
            render('subdivisions/index.json.jbuilder').html_safe
       %>,
    "years": <%= @years.to_json %>
  }
</script>
<% end %>

<script>
if (window.CarListing) {
  var context = window.CarListing;
}
else {
  var context = window;
}
context.searchParams = <%= @search_params.to_json.html_safe %>;
</script>
