<% cache(expires_in: 10.minutes, race_condition_ttl: 10.seconds) do %>
<script type="application/json" id="bootstrapped-listings">
  {"listings":
      <%=
            render('listings/index.json.jbuilder').html_safe %>,
   "subdivisions":
      <%=
            render('subdivisions/index.json.jbuilder').html_safe
       %>,
    "years": <%= @years.to_json %>,
    "sortOptions": <%= Listing::SORT_OPTIONS.to_json.html_safe %>
  }
</script>
<% end %>

<script>
  CarListing.searchParams = <%= @search_params.to_json.html_safe %>;
  CarListing.currentUserID = <%= current_user.try(:id) || 'null' %>;
</script>
